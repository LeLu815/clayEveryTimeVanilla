extends ../../base.pug

block content 
  div
    form(method="post") 
      input(type="text", name="title", placeholder="제목", value=`${queryPlaygroundContentData.title}`)
      input(type="text", name="image", placeholder="이미지")
      textarea(name="mainText", cols="30", rows="10", placeholder="내용을 입력하세요") #{queryPlaygroundContentData.mainText}
      input(type="submit", value="수정")