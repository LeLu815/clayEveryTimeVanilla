mixin profileContentItem(item)
  a(href=`/content/playground/${item.newContentOwnName}`)  
    div 여기에 이미지
    div
      div 
        div 유저 사진 여기
        div #{item.owner.displayName}
      div 
        div #{item.title} 
        div #{item.mainText}
      div
        div #{item.createdAt}
        div #{item.comment}
        div #{item.likes}
  div 
    div 삭제
    input(type="checkbox", name="isDel", value=`${item.newContentOwnName}`)

